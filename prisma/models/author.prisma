model Author {
  author_id          Int             @id @default(autoincrement())
  author_email       String          @unique
  author_fname       String?
  author_lname       String?
  author_designation String?
  author_password    String?         @db.VarChar(500)
  IntoArticle        IntoArticle[]
  ArticleAuthor      ArticleAuthor[]
}
